{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 300, "height": 200,
  "data": {"url": "https://raw.githubusercontent.com/MaxTuu/MaxTuu.GitHub.io/main/ECO/deaths_ages.csv"},
  "mark": "area",
  "encoding": {
    "x": {
      "timeUnit": "yearmonth", "field": "Year",
      "axis": {"format": "%Y"}
    },
    "y": {
      "aggregate": "sum", "field": "Deaths Per Capita"
    },
    "color": {
      "field": "Age",
      "scale": {"scheme": "category20b"},
      "sort": {"field": "order"}
    }
  }
}
