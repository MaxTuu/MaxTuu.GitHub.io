{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Most popular days for marriage",
  "data": {
    "url": "https://raw.githubusercontent.com/MaxTuu/MaxTuu.GitHub.io/main/ECO/fertility_ParentsAge.csv"
  },
  "mark": {"type":"rect", "tooltip": true},
  "encoding": {
    "y": {
      "field": "father age",
      "type": "nominal",
      "scale": {
        "domain": [
          "Under 20",
          "20 to 24 ",
          "25 to 29 ",
          "30 to 34 ",
          "35 to 39 ",
          "40 to 44 ",
          "45 to 49 ",
          "50 to 54 ",
          "55 to 59 ",
          "60 to 64 ",
          "65 and over"
        ]
      }
    },
    "x": {
      "field": "mother age",
      "type": "nominal",
      "scale": {
        "domain": [
          "Under 20",
          "20 to 24 ",
          "25 to 29 ",
          "30 to 34 ",
          "35 to 39 ",
          "40 to 44 ",
          "45 and over"
        ]
      }
    },
    "color": {"field": "births", "type": "quantitative"}
  },
  "config": {"axis": {"grid": true, "tickBand": "extent"}}
}
